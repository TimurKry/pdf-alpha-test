{
    "version": 2,
    "builds": [
      {
        "src": "PDF_reading_and_filling/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "Home/**",
        "use": "@vercel/static"
      },
      {
        "src": "Admin/**",
        "use": "@vercel/static"
      },
      {
        "src": "css/**",
        "use": "@vercel/static"
      },
      {
        "src": "js/**",
        "use": "@vercel/static"
      },
      {
        "src": "images/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/Home/index.html"
      },
      {
        "src": "/admin",
        "dest": "/Admin/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/PDF_reading_and_filling/server.js"
      },
      {
        "src": "/css/(.*)",
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "/js/$1"
      },
      {
        "src": "/images/(.*)",
        "dest": "/images/$1"
      }
    ]
  }
  